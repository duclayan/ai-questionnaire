{
    "navigationFallback": {
      "rewrite": "/index.html"
    },

    "globalHeaders": {
    "Access-Control-Allow-Origin": "https://dduclayan-backend-dme4fxabfcfkg6a9.eastus-01.azurewebsites.net",
    "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
    "Access-Control-Allow-Headers": "Content-Type"
    },
    "routes": [
        {
            "route": "/api/*",
            "allowedRoles": ["anonymous"],
            "serve": "/index.html"
          },
      {
        "route": "/",
        "serve": "/index.html",
        "statusCode": 200
      },
      {
        "route": "/form",
        "serve": "/index.html",
        "statusCode": 200
      },
      {
        "route": "/dashboard",
        "serve": "/index.html",
        "statusCode": 200
      },
      {
        "route": "/project/:id",
        "serve": "/index.html",
        "statusCode": 200
      },
      {
        "route": "/login",
        "serve": "/index.html",
        "statusCode": 200
      }
    ]
  }

